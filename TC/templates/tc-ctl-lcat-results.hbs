{{#each servicesFound}}
    {{#if (gt ../servicesLooked 1)}}
<li class="tc-ctl-lcat-search-group {{#if service.isCollapsed}}tc-collapsed{{/if}}" data-service-id="{{service.id}}">
    <h5>{{service.title}}</h5><ul>
    {{/if}}
        {{#each founds}}
        <li data-layer-name="{{Name}}" {{#if alreadyAdded}} class="tc-checked"{{/if}}>
            <h5 class="tc-selectable" {{#if alreadyAdded}} data-tooltip="[[layerAlreadyAdded]]" {{else}} data-tooltip="[[clickToAddToMap]]" {{/if}}>
                {{#if parent.parent}}
                    {{> tc-ctl-lcat-results-path layer=./parent}}
                {{/if}}
                {{Title}}
            </h5>
        <input type="checkbox" class="tc-toggle tc-ctl-lcat-search-btn-info" title="[[infoFromThisLayer]]"></input>
        </li>
	    {{/each}}
    {{#if (gt ../servicesLooked 1)}}
</ul></li>
    {{/if}}
{{else}}
<li class="tc-ctl-lcat-no-results"><h5>[[noMatches]]</h5></li>
{{/each}}
